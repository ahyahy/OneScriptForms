		|
		|        public AutoHeaderHeightLayout(TreeViewAdv treeView, int headerHeight)
		|        {
		|            _treeView = treeView;
		|            PreferredHeaderHeight = headerHeight;
		|            _measureContext = new DrawContext();
		|            _measureContext.Graphics = Graphics.FromImage((Image)new Bitmap(1, 1));
