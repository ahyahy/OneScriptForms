	Стр = ТекстДок.ПолучитьТекст();
	//находим текст таблицы
	СтрТаблица = СтрНайтиМежду(Стр, "<H3 class=dtH3>Перечисления</H3>", "</TBODY></TABLE>", Ложь, );
	// Сообщить("==================" + СтрТаблица[0]);
